{
  "default": {
    "paths": ["features/loginPage.feature"],
    "publishQuiet": true,
    "dryRun": false,
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "require": [
      "src/steps/**/*.ts",
      "src/support/**/*.ts"
    ],
    "requireModule": ["ts-node/register"],
    "format": [
      "json:reports/cucumber_report.json",
     "progress",
     "html:reports/cucumber_report.html"
    ]
  }
}
